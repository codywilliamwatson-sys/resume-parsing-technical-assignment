{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run PDF Parser Example",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/parse_pdf_resume.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "python": "${workspaceFolder}/venv/bin/python",
            "justMyCode": true
        },
        {
            "name": "Run Word Parser Example",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/examples/parse_word_resume.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            },
            "python": "${workspaceFolder}/venv/bin/python",
            "justMyCode": true
        }
    ]
}

